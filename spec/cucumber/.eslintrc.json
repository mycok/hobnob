{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "func-names": "off",
        "prefer-arrow-callback": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "global-require": "off"
    }
}
